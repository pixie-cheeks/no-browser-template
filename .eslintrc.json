{
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["disable-autofix"],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "prefer-const": "off",
    "disable-autofix/prefer-const": "warn",
    "lines-between-class-members": "off",
    "import/extensions": "off"
  }
}
